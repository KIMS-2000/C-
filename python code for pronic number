def isPronicNumber(num):    
    flag = False;    
        
    for i in range(1, num+1):    
#Checks for pronic number by multiplying consecutive numbers    
        if((i*(i+1)) == num):    
            flag = True;    
            break;    
    return flag;    
     
#Displays pronic numbers between 1 and 1000
for j in range(1, 1001):    
    if(isPronicNumber(j)):    
        print(j);    
        print(" ");
